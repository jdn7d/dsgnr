<h1> Sign In </h1>

<%= form_tag '/signin' do  %>

   <%= label_tag :email %> 
   <%= email_field_tag :email %> <br> 

   <%= label_tag :password %> 
   <%= password_field_tag :password %> <br>

   <%= submit_tag "Sign In" %> <br>
   <%= link_to "Signin With Google", '/auth/google_oauth2' %> <br>
   <%= link_to "Create an account", new_designer_path %> <br>

<% end %>
